{
	/BuildQueryTest
	{
		{
			/Path		"/a"
			/Query		"alpha=beta"
			/Expected
			{
				a
				/alpha	beta
			}
		}
		{
			/Path		"/a"
			/Query		"alpha=beta+3"
			/Expected
			{
				a
				/alpha	"beta 3"
			}
		}
		{
			/Path		"/fda"
			/Query		"X=ABC&foo=bah&zzz"
			/Expected
			{
				fda
				/X		ABC
				/foo	bah
				zzz
			}
		}
		{
			/Path		"/finVAL/XS.ISA/foo/bah"
			/Query		"action=Go&role=Guest"
			/Expected
			{
				finVAL
				XS.ISA
				foo
				bah
				/action	Go
				/role	Guest
			}
		}
		{
			/Path		"/"
			/Query		""
			/Expected	*
		}
	}
