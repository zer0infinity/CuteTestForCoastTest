{

	/InitFinisNoModuleConfigTest {
		/ModuleConfig {
		}
	}
	/InitFinisDefaultsTest {
		/ModuleConfig {
			/QueueWorkingModule {
			}
		}
	}
	/InitFinisTest {
		/ModuleConfig {
			/QueueWorkingModule {
				/QueueSize	25
				/UsePoolStorage			1			# optional, [0|1], default 0, use preallocated memory pool for storage of Queue elements
				/PoolStorageSize		1024		# optional, [kB], default 10240, pool storage size in kbytes
				/NumOfPoolBucketSizes	10			# optional, default 10, number of different allocation units within PoolStorage, starts at 16 bytes and always doubles the size so 16 << 10 will give a max block size of 16384 bytes
				/Logging {
					/Servername	NonExistingServer
				}
			}
		}
	}
	/GetAndPutbackTest {
		/ModuleConfig {
			/QueueWorkingModule {
				/QueueSize	1
				/UsePoolStorage			1			# optional, [0|1], default 0, use preallocated memory pool for storage of Queue elements
				/PoolStorageSize		1024		# optional, [kB], default 10240, pool storage size in kbytes
				/NumOfPoolBucketSizes	10			# optional, default 10, number of different allocation units within PoolStorage, starts at 16 bytes and always doubles the size so 16 << 10 will give a max block size of 16384 bytes
				/Logging {
					/Servername	Server
				}
			}
		}
	}
}
