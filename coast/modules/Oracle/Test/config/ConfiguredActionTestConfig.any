{
	/DLL {
		CoastAppLog
		CoastDataAccess
		CoastRenderers
	}
	/Modules {
		ActionsModule
		RenderersModule
		OracleModule
		DataAccessImplsModule
		MappersModule
		AppLogModule
		ServersModule
	}

	/Servers {}
	# uncomment the server slot for enabling TimeLogging
	/Server	Server

	/Actions {
		/CallDataAccessAction	{ CallDA }
	}
	/Renderers {
		/SwitchRenderer 		{ Switch }
		/ContextLookupRenderer 	{ Lookup }
	}
	/OracleModule {
		/ConnectionPool {
			/ParallelQueries		4
			/CloseConnectionTimeout	20
		}
	}

	/DataAccessImpls {
		/OracleDAImpl {
			IncompleteParamsDA
			/SifsCoastOrcl {
				SelectEmployees
				SelectEmployeesTitlesOnce
				SPMaxSalary
				/SPExecute {
					SPExecuteArray
				}
				LoadOratestSchema
				UpdateEmployees
				SimpleSQLAccess
			}
		}
		/MapperTestDAImpl {
			/OracleSPMapperSlotTest {
				SPRenderedOracleSPMapperParamTest
				OracleResultMapperArrayIdxTest
			}
		}
	}

	/Mappers {
		/Input {
			/ParameterMapper {
				/SifsCoastOrcl {
					/SelectEmployees {
						SelectEmployeesTitlesOnce
						UpdateEmployees
						SimpleSQLAccess
					}
					LoadOratestSchema
				}
			}
			/OracleParameterMapper {
				IncompleteParamsDA
				/SifsCoastOrclSP {
					SPMaxSalary
					/SPExecute {
						SPExecuteArray
					}
				}
				/OracleSPMapperSlotTest {
					SPRenderedOracleSPMapperParamTest
				}
				OracleResultMapperArrayIdxTest
			}
		}
		/Output {
			/ResultMapper {
			}
			/OracleResultMapper {
				AppendResultMapper
				/PutResultMapper {
					/OracleSPMapperSlotTest {
						SPRenderedOracleSPMapperParamTest
					}
				}
				/OracleDAImpl {
					SelectEmployees
					SelectEmployeesTitlesOnce
					SPMaxSalary
					/SPExecute {
						IncompleteParamsDA
						SPExecuteArray
					}
					LoadOratestSchema
					UpdateEmployees
					SimpleSQLAccess
				}
				OracleResultMapperArrayIdxTest
			}
		}
	}

