						}
					}
				}
				{
					/Set
					{
						/Data
						{
							/PoolId		"ForthConnection"
							/LDAPMaxConnections		1
						}
						/Expected
						{
							/Return				1
						}
					}
				}
			}
		}
		# 15 Getting this handle, not  valid rebind timeout
		{
			/NumberOfRuns 1
			/Suite
			{
				{
					/Get
					{
						/Data
						{
							/PoolId		"ForthConnection"
